{
  "name": "@animoca/template-ethereum-migrations",
  "version": "0.0.3",
  "description": "description",
  "author": "author",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:animoca/template-ethereum-migrations.git"
  },
  "scripts": {
    "postinstall": "bash -c 'for cfg in .vscode/settings.json .vscode/extensions.json .vscode/launch.json; do cp -n ${cfg}.default ${cfg} || :; done'",
    "lint:js": "eslint \"**/*.{js,ts}\" --no-error-on-unmatched-pattern",
    "lint": "run-s lint:js",
    "fix:js": "eslint --fix \"**/*.{js,ts}\" --no-error-on-unmatched-pattern",
    "fix": "run-s fix:js",
    "format:js": "prettier --write \"**/*.{ts,js}\"",
    "format": "run-s format:js",
    "deploy": "cross-env NODE_OPTIONS=--max-old-space-size=8192 hardhat deploy --show-stack-traces",
    "deploy:test": "cross-env NETWORK_TAGS=test yarn deploy",
    "verify": "hardhat etherscan-verify --show-stack-traces",
    "clean:vscode": "rm -rf .vscode/extensions.json .vscode/launch.json .vscode/settings.json || :",
    "clean:cache": "rm -rf cache || :",
    "clean:deployments": "rm -rf deployments/localhost* deployments/hardhat* || :",
    "clean:generated": "rm -rf generated/localhost* generated/hardhat* || :",
    "clean:hardhat": "rm -rf hardhat.config.last.json || :",
    "clean:etherscan": "rm -rf etherscan_requests || :",
    "clean:migrations": "run-s clean:cache clean:deployments clean:generated clean:hardhat clean:etherscan",
    "clean": "run-s clean:migrations",
    "run-all:migrations": "run-s format fix clean deploy deploy:test",
    "run-all": "run-s run-all:migrations",
    "prepack:migrations": "run-s clean:migrations",
    "prepack": "run-s prepack:migrations"
  },
  "devDependencies": {
    "@animoca/ethereum-contract-helpers": "^1.0.2",
    "@animoca/ethereum-contracts": "^3.0.0",
    "@animoca/ethereum-contracts-1.0": "npm:@animoca/ethereum-contracts@1.1.1",
    "@animoca/ethereum-contracts-2.0": "npm:@animoca/ethereum-contracts@2.0.0",
    "@animoca/ethereum-contracts-3.0": "npm:@animoca/ethereum-contracts@3.0.0",
    "@animoca/ethereum-contracts-bridging-0.3": "npm:@animoca/ethereum-contracts-bridging@0.3.0",
    "@animoca/ethereum-contracts-bridging-1.0": "npm:@animoca/ethereum-contracts-bridging@1.0.0",
    "@animoca/ethereum-migrations": "^3.0.2",
    "@animoca/template-ethereum-contracts-0.0": "npm:@animoca/template-ethereum-contracts@0.0.6",
    "@nomicfoundation/hardhat-ethers": "^3.0.4",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "cli-progress": "^3.12.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "ethers": "^6.8.0",
    "hardhat": "^2.18.3",
    "hardhat-deploy": "^0.11.43",
    "hardhat-deploy-ethers": "^0.4.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "yesno": "^0.4.0"
  }
}
